{
  "name": "xenioushk/kb_display_as_blog_post",
  "description": "This Addon automatically include KB posts in the blog listings according to date order.",
  "type": "project",
  "autoload": {
    "psr-4": {
      "KBDABP\\": "includes/"
    }
  },
  "require": {
    "xenioushk/bwl-plugin-api": "^1.0"
  },
  "require-dev": {
    "wp-coding-standards/wpcs": "^3.1",
    "phpcompatibility/phpcompatibility-wp": "2.1",
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
    "squizlabs/php_codesniffer": "^3.10"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "johnpbloch/wordpress-core-installer": true
    }
  },
  "authors": [
    {
      "name": "Mahbub Alam Khan",
      "email": "hkhan.cse@gmail.com"
    }
  ],
  "minimum-stability": "stable"
}
