{
  "name": "kb-display-as-blog-post",
  "version": "2.0.1",
  "description": "This Addon automatically include KB posts in the blog listings according to date order.",
  "main": "index.js",
  "scripts": {
    "build": "wp-scripts build",
    "start": "wp-scripts start",
    "dev": "wp-scripts start",
    "start:hot": "wp-scripts start --hot",
    "test": "echo \"Error: no test specified\" && exit 1",
    "plugin-zip": "wp-scripts plugin-zip",
    "make-pot": "wp i18n make-pot . languages/kb-display-as-blog-post.pot --slug=kb-display-as-blog-post --domain=bkb-kbdabp --exclude=node_modules,src,vendor",
    "make-json": "wp i18n make-json languages/ --no-purge",
    "lint:php": "vendor/bin/phpcs",
    "lint:phpn": "vendor/bin/phpcs -n",
    "lint:php:fix": "vendor/bin/phpcbf"
  },
  "author": "Md Mahbub Alam Khan <hkhan.cse@gmail.com> (https://bluewindlab.net/)",
  "license": "MIT",
  "dependencies": {
    "@wordpress/scripts": "24.6.0"
  }
}
